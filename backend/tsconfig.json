{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "lib": [
            "esnext"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "outDir": "lib",
        "sourceMap": true,
        "target": "esnext"
    },
    "include": [
        "src/services/**/*",
    ],
    "exclude": [
        "node_modules",
    ]
}